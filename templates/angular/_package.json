      <% } %>
      "angular-mocks": "github:angular/bower-angular-mocks@1.3.15",
      "angular-resource": "github:angular/bower-angular-resource@1.4.7",
      "angular-ui-router": "github:angular-ui/ui-router@0.2.15",
