    "es6",
    "jspm",
    "gulp",
    "babel",
    "systemjs",
    "angular"
  ],
  "directories": {
    "baseURL": "src",
    "test": "test"
  },
  "devDependencies": {
    "babel-core": "6.1.4",
    "babel-eslint": "6.1.2",
    "browser-sync": "2.9.8",
    "chai": "3.3.0",
    "chai-as-promised": "5.1.0",
    "connect-history-api-fallback": "1.1.0",
    "del": "2.0.2",
    "eslint-config-standard": "4.4.0",
    "eslint-plugin-standard": "1.3.1",
    "gulp": "3.9.0",
    "gulp-autoprefixer": "3.0.2",
    "gulp-cached": "1.1.0",
    "gulp-changed": "1.3.0",
