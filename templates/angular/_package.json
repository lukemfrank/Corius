    "karma-sinon-chai": "1.1.0",
    "karma-verbose-reporter": "0.0.3",
    "lodash": "3.10.1",
    "mocha": "2.3.3",
    "object-assign": "4.0.1",
    "require-dir": "0.3.0",
    "run-sequence": "1.1.4"
