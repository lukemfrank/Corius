  "jspm": {
    "directories": {
      "baseURL": "src",
      "test": "test",
      "packages": "src/jspm_packages"
    },
    "configFile": "src/system.config.js",
    "dependencies": {
      "angular": "github:angular/bower-angular@1.3.15",
      <% if (uiFramework === 'materialdesign') { %>
