{
  "name": "",
  "version": "",
  "description": "",
  "main": "",
  "author": "",
  "license": "",
  "scripts": {
    "test": "karma start",
    "test:watch": "karma start --no-single-run --auto-watch",
    "dev": "export BUILD_ENV=dev && gulp",
    "staging": "export BUILD_ENV=staging && gulp build",
    "build": "export BUILD_ENV=prod && gulp build",
    "buildJs": "jspm bundle-sfx js/app dist/app.min.js --minify --skip-source-maps",
    "postinstall": "cd src && jspm install"
  },
  "keywords": [
    "es6",
    "jspm",
    "gulp",
    "babel",
    "systemjs"
  ],
  "directories": {
    "baseURL": "src",
    "test": "test"
  },
  "devDependencies": {
    "babel-core": "^6.1.4",
    "babel-eslint": "6.1.2",
    "browser-sync": "^2.9.8",
    "chai": "^3.3.0",
    "chai-as-promised": "^5.1.0",
    "connect-history-api-fallback": "^1.1.0",
    "del": "^2.0.2",
    "eslint-config-standard": "4.4.0",
    "eslint-plugin-standard": "1.3.1",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.0.2",
    "gulp-cached": "^1.1.0",
    "gulp-changed": "^1.3.0",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^2.2.0",
    "gulp-eslint": "^1.0.0",
    "gulp-imagemin": "^2.3.0",
    <% if (cssFramework === 'less') { %>
    "gulp-less": "^3.0.3",
    "less": "2.5.3",
    <% } %>
    "gulp-minify-css": "^1.2.1",
    "gulp-minify-html": "^1.0.4",
    "gulp-plumber": "^1.0.1",
    <% if (cssFramework !== 'sass') { %>
    "gulp-recess": "^1.1.2",
    <% } %>
    "gulp-replace": "^0.5.4",
    "gulp-replace-task": "^0.11.0",
    <% if(cssFramework === 'sass') { %>
    "gulp-sass": "^2.0.4",
    "gulp-scss-lint": "^0.3.4",
    <% } %>
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.4.1",
    "gulp-util": "^3.0.6",
    "gulp-yaml": "1.0.1",
    "imagemin-pngquant": "^4.2.0",
    "jspm": "0.16.39",
    "karma": "^0.13.19",
    "karma-babel-preprocessor": "^5.2.2",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.5.2",
    "karma-jspm": "^2.0.1",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sinon-chai": "^1.1.0",
    "karma-verbose-reporter": "0.0.3",
    "lodash": "^3.10.1",
    "mocha": "^2.3.3",
    "object-assign": "^4.0.1",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.4"
  },
  "jspm": {
    "directories": {
      "baseURL": "src",
      "test": "test",
      "packages": "src/jspm_packages"
    },
    "configFile": "src/system.config.js",
    "dependencies": {
      <% if(uiFramework === 'bootstrap') { %>
      "bootstrap": "github:twbs/bootstrap@^3.3.5",
      <% } %>
      <% if(uiFramework === 'materialdesign') { %>
      "dogfalo/materialize": "github:dogfalo/materialize@master",
      <% } %>
      "clean-css": "npm:clean-css@^3.4.5",
      "console-polyfill": "npm:console-polyfill@0.2.2",
      "css": "github:systemjs/plugin-css@^0.1.20",
      "director": "npm:director@^1.2.8",
      "es6-shim": "npm:es6-shim@0.35.0",
      "font-awesome": "npm:font-awesome@4.4.0",
      "history": "npm:history@^1.12.2",
      "jquery": "github:components/jquery@^2.1.4",
      "normalize.css": "github:necolas/normalize.css@^3.0.3",
      "text": "github:systemjs/plugin-text@^0.0.2",
      "whatwg-fetch": "npm:whatwg-fetch@^0.9.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    },
    "overrides": {
      <% if(uiFramework === 'materialdesign') { %>
      "github:dogfalo/materialize@master": {
        "//": "Why the override? See https://github.com/Dogfalo/materialize/issues/2290#issuecomment-165110728",
        "main": "bin/materialize.js",
        "shim": {
          "bin/materialize.js": [
            "jquery"
          ]
        },
        "format": "global",
        "dependencies": {
          "jquery": "*"
        },
        "registry": "npm"
      }
      <% } %>
    }
  }
}
